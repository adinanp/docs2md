Gem::Specification.new do |s|
  s.name        = "docs2md"
  s.version     = "0.1.0"
  s.description = "Simple gem to get and convert google docs for Markdown"
  s.summary     = "Convert to Markdown"
  s.author      = "Adinan Paiva"
  s.files       = `git ls-files`.split($\)
  s.executables = ["main"]
end
